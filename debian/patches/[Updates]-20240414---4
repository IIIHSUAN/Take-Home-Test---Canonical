+  GNULIB_WCSSTR=0;      
+  GNULIB_WCSTOK=0;      
+  GNULIB_WCSWIDTH=0;    
+    HAVE_BTOWC=1;         
+  HAVE_MBSINIT=1;       
+  HAVE_MBRTOWC=1;       
+  HAVE_MBRLEN=1;        

+printf "%s\n" "$gl_cv_func_malloc_posix" >&6; }
+
+
+  GNULIB_BTOWC=0;       
+  GNULIB_WCTOB=0;       
+  GNULIB_MBSINIT=0;     
+  GNULIB_MBRTOWC=0;     
+  GNULIB_MBRLEN=0;      
+  GNULIB_MBSRTOWCS=0;   
+  GNULIB_MBSNRTOWCS=0;  
+  GNULIB_WCRTOMB=0;     

+  HAVE_MBSNRTOWCS=1;    
+  HAVE_WCRTOMB=1;       
+  HAVE_WCSRTOMBS=1;     
+  HAVE_WCSNRTOMBS=1;    
+  HAVE_WMEMCHR=1;       
+  HAVE_WMEMCMP=1;       
+  HAVE_WMEMCPY=1;       
+  HAVE_WMEMMOVE=1;      
+  HAVE_WMEMSET=1;       
+  HAVE_WCSLEN=1;        
+  HAVE_WCSNLEN=1;       
+  HAVE_WCSCPY=1;        
+  HAVE_WCPCPY=1;        
+  HAVE_WCSNCPY=1;       
+  HAVE_WCPNCPY=1;       
+  HAVE_WCSCAT=1;        
+  HAVE_WCSNCAT=1;       
+  HAVE_WCSCMP=1;        
+  HAVE_WCSNCMP=1;       
+  HAVE_WCSCASECMP=1;    
+  HAVE_WCSNCASECMP=1;   
+  HAVE_WCSCOLL=1;       
+  HAVE_WCSXFRM=1;       
+  HAVE_WCSDUP=1;        
+  HAVE_WCSCHR=1;        
+  HAVE_WCSRCHR=1;       
+  HAVE_WCSCSPN=1;       
+  HAVE_WCSSPN=1;        
+  HAVE_WCSPBRK=1;       
+  HAVE_WCSSTR=1;        
+  HAVE_WCSTOK=1;        
+  HAVE_WCSWIDTH=1;      
+  HAVE_DECL_WCTOB=1;    
+  HAVE_DECL_WCWIDTH=1;  
+  REPLACE_MBSTATE_T=0;  
+  REPLACE_BTOWC=0;      
+  REPLACE_WCTOB=0;      
+  REPLACE_MBSINIT=0;    
+  REPLACE_MBRTOWC=0;    
+  REPLACE_MBRLEN=0;     
+  REPLACE_MBSRTOWCS=0;  
+  REPLACE_MBSNRTOWCS=0; 
+  REPLACE_WCRTOMB=0;    

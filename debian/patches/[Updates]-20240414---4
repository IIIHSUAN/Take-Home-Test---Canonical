+ac_fn_c_check_type "$LINENO" "mode_t" "ac_cv_type_mode_t" "$ac_includes_default"
+if test "x$ac_cv_type_mode_t" = xyes
+then :
+  
+else $as_nop
+  
+printf "%s\n" "@%:@define mode_t int" >>confdefs.h
+
+fi
+

+  GNULIB_WCSCAT=0;      
+  GNULIB_WCSNCAT=0;     
+  GNULIB_WCSCMP=0;      
+  GNULIB_WCSNCMP=0;     
+  GNULIB_WCSCASECMP=0;  
+  GNULIB_WCSNCASECMP=0; 
+  GNULIB_WCSCOLL=0;     
+  GNULIB_WCSXFRM=0;     
+  GNULIB_WCSDUP=0;      
+  GNULIB_WCSCHR=0;      
+  GNULIB_WCSRCHR=0;     
+  GNULIB_WCSCSPN=0;     
+  GNULIB_WCSSPN=0;      
